{
  "name": "authserver",
  "version": "1.0.0",
  "description": "Server that provides authentication",
  "main": "src/index.ts",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "db:migrate": "ts-node node_modules/typeorm/cli.js migration:run",
    "test": "ts-node jasmine-test.ts",
    "test-coverage": "nyc npm run test"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/tests/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ]
  },
  "dependencies": {
    "@types/lodash": "^4.14.168",
    "@types/morgan": "^1.9.2",
    "bcrypt": "^5.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.32",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/jasmine": "^3.6.9",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "jasmine": "^3.7.0",
    "jasmine-reporters": "^2.4.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.2",
    "karma": "^6.3.2",
    "karma-typescript": "^5.5.1",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended"
    ]
  }
}
